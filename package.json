{
  "name": "orbit-pdf",
  "version": "1.0.1",
  "description": "CLI tool for converting Orbit html resume to pdf",
  "author": "Željko Šević",
  "bin": {
    "orbit-pdf": "./bin/run"
  },
  "bugs": "https://github.com/zsevic/orbit-pdf/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "puppeteer": "^1.14.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "globby": "^8",
    "mocha": "^5",
    "mock-require": "^3.0.3",
    "nyc": "^14.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/zsevic/orbit-pdf",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "orbit-pdf",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "zsevic/orbit-pdf",
  "scripts": {
    "deploy": "npm publish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint --fix --ignore-path .eslintignore .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc --reporter=html mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@zsevic"
  }
}
